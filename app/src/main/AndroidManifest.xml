<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.suishouji">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_avatar_asking"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CostActivity" />
        <activity android:name=".activity.menu.AddAccountBookActivity" />
        <activity android:name=".activity.menu.AddAccountBookDetailActivity" />
        <activity android:name=".activity.menu.FaceBoardActivity" />
        <activity android:name=".activity.menu.FaceBoardRecordActivity"></activity>
    </application>

</manifest>